main {
    Struct Set {
        List<?> data;
    }
    impl Set {
        # adiciona elemento e retorna o pr√≥prio Set (permite encadeamento)
        function add(? value) {
        int i = 0;
        while(i < s.data.size()) {
            if(s.data.get(i) == value){
            return s;
            }
            i++;
        }
         s.data.add(value);
            return s;
        }

        function remove( int index) {
        s.data.remove(index);
        return s;
        }

    }

    # ==== uso ====
