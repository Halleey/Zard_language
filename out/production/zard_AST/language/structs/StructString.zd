main {

    Struct Set {
        List<string> data;
    }

    # Cria uma inst√¢ncia do Set
    Set s;

    print("digite seu nome");
    string nome = input();
    string x = "zhum";
    # Adiciona elementos na lista interna
    s.data.add("1");
    s.data.add(nome);
    s.data.add(x);

    print("Elementos do Set:");
    print(s.data);

    # Usando size()
    print("Tamanho:");
    print(s.data.size());

    #Usando get
    print("get");
    print(s.data.get(0));

    #Usando remove
    print("remove");
    s.data.remove(0);
    print(s.data);

    #limpa lista
    print("apos limpeza");
    s.data.clear();
    print(s.data);

}
