import "src/language/stdlib/t.zd" as math;

main {

    print(math.sum(3.0, 4.0));

    print(math.sub(10.0, 6.0));

    print(math.mul(2.0, 5.0));

    print(math.div(10.0, 2.0));
    print(math.div(5.0, 0.0));

    print(math.pow(2.0, 3));
    print(math.pow(5.0, -1));

    print(math.sqrt(16.0));
    print(math.sqrt(-4.0));

    print(math.sin(0.5));

    print(math.cos(0.5));

    print(math.factorial(5));
    print(math.fact(6));
