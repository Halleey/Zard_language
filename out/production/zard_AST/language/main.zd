
main {
    int i = 0;

    while (i < 5) {
        int j = 0;

        while (j < 5) {
            print("i :");
            print(i);
            print("j :");
            print(j);

            if (j == 2) {
                print("Break do loop interno (j)");
                break;
            }

            j = j + 1;
        }

        print("Fim do loop interno para i = " );
        print(i);

        if (i == 3) {
            print("Break do loop externo (i)");
            break;
        }

        i = i + 1;
    }
