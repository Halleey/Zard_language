main {
    Struct Set {
        List<?> data;
    }
    impl Set {
        # adiciona elemento e retorna o pr√≥prio Set (permite encadeamento)
        function Struct<Set> add(Set s, ? value) {
            s.data.add(value);
            return s;
        }
    }

    # ==== uso ====
    Set s;
    Set chars;
    Set names;
    Set t = {data : 3,4,5};
    s.add(1);

    names.add(3.14);
    print(names);
    print(t);
    print(s);
}
