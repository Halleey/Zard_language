main {
    Struct Set {
        string name;
        List<?> data;
    }
    impl Set {
        # adiciona elemento e retorna o pr√≥prio Set (permite encadeamento)
        function Struct<Set> add(Struct<Set> s, int value) {
            s.data.add(value);
            return s;
        }
    }

    # ==== uso ====

     Set t = {name : "zard", data : 3,4,5};
    # adiciona elementos (sem this, passando s explicitamente)
    t.add(2);
    print(t);
}
