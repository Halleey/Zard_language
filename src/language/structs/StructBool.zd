main {

    Struct Set {
        List<boolean> data;
    }

    # Cria uma inst√¢ncia do Set
    Set s;


    # Adiciona elementos na lista interna
    s.data.add(true);
    s.data.add(false);
    s.data.add(true);

    print("Elementos do Set:");
    print(s.data);

    # Usando size()
    print("Tamanho:");
    print(s.data.size());

    #Usando get
    print("get");
    print(s.data.get(0));

    #Usando remove
    print("remove");
    s.data.remove(0);
    print(s.data);

    #limpa lista
    print("apos limpeza");
    s.data.clear();
    print(s.data);

}
