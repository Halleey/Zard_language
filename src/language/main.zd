function pow(double base, int exp) {
    if (exp < 0) {
        print("Aviso: expoente negativo no suportado, retornando 0");
        return 0;
    }
    double result = 1;
    int i = 0;
    while (i < exp) {
        result = result * base;
        i++;
    }
    return result;
}
print(pow(2.0, 1));

